                        http://www.eclipse.org/schema/score
                        http://www.eclipse.org/schema/score/score.xsd">

    <bean class="com.hp.score.lang.runtime.configuration.SlangRuntimeSpringConfig"/>
    <bean class="com.hp.score.lang.compiler.configuration.SlangCompilerSpringConfig"/>

    <score:engine />

    <score:worker uuid="-1"/>

</beans>